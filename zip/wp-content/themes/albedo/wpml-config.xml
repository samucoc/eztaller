<wpml-config>
	<admin-texts>

		<!-- fw_theme_settings_options:YOURTHEME (this is the field in the database)-->
		<key name="fw_theme_settings_options:wplab-albedo">
			<!-- Page 404 -->
			<key name="page_404_title_1"/>
			<key name="page_404_title_2"/>
			<key name="page_404_title_3"/>
			<key name="page_404_display_home_btn">
			<key name="yes">
				<key name="page_404_home_btn_text"/>
			</key>
		</key>

		<!-- Header -->
		<key name="top_bar_enabled">
			<key name="yes">
				<key name="top_bar_elements">
					<key name="text">
						<key name="content"/>
					</key>
					<key name="text_icon">
						<key name="content"/>
					</key>
				</key>
			</key>
		</key>

		<key name="header_cta">
			<key name="yes">
				<key name="header_cta_button_text"/>
				<key name="header_cta_button_url"/>
			</key>
		</key>

		<key name="menu_style">
			<key name="minimal">
				<key name="submenu_minimal_free_text"/>
			</key>
		</key>

		<key name="menu_cta">
			<key name="yes">
				<key name="menu_cta_button_text"/>
				<key name="menu_cta_button_url"/>
			</key>
		</key>

		<!-- Footer -->
		<key name="footer_bar">
			<key name="yes">
				<key name="footer_bar_text"/>
			</key>
		</key>

		<!-- Portfolio -->
		<key name="portfolio_single_display_similar_posts">
			<key name="yes">
				<key name="portfolio_single_similar_posts_title"/>
				<key name="portfolio_single_similar_posts_desc"/>
			</key>
		</key>

		<!-- WooCommerce -->
		<key name="shop_title"/>
	</key>

	</admin-texts>

	<custom-types>
		<custom-type translate="1">fw-portfolio</custom-type>
		<custom-type translate="1">benefits</custom-type>
		<custom-type translate="1">polls</custom-type>
	</custom-types>

	<taxonomies>
		<taxonomy translate="1">fw-portfolio-category</taxonomy>
		<taxonomy translate="1">benefits_category</taxonomy>
	</taxonomies>

</wpml-config>
