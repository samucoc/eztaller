{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "ext-soap": "*",
        "barryvdh/laravel-cors": "^0.11.4",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "2.*",
        "tymon/jwt-auth": "dev-develop",
        "webpatser/laravel-uuid": "^3.0",
        "yajra/laravel-oci8": "5.5.*"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Yajra\\Oci8\\Query\\Grammars\\": "overrides/yajra/laravel-pdo-via-oci8/src/Oci8/Query/Grammars",
            "Illuminate\\Database\\Eloquent\\Relations\\": "overrides/laravel/framework/src/Illuminate/Database/Eloquent/Relations",
            "Illuminate\\Database\\Query\\": "overrides/laravel/framework/src/Illuminate/Database/Query",
            "Illuminate\\Database\\Eloquent\\Concerns\\": "overrides/laravel/framework/src/Illuminate/Database/Eloquent/Concerns"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
